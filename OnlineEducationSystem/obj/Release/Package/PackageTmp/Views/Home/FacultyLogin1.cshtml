@model OnlineEducationSystem.Models.FacultyViewModel
@{
    ViewBag.Title = "FacultyLogin1";
}


<div class="container" style="width:60%;margin-top:2%">
    @if (Model != null)

    {
    <ul class="list-group">
        <li class="list-group-item active">Faculty Information</li>
        <li class="list-group-item">Faculty Id:<b>@Model.FId</b></li>
        <li class="list-group-item">Faculty Name:<b>@Model.FName</b></li>
        <li class="list-group-item">Faculty Branch:<b>@Model.FBranch</b></li>
        <li class="list-group-item">Faculty MobileNumber:<b>@Model.MobileNumber</b></li>
        <li class="list-group-item">Faculty Email:<b>@Model.Email</b></li>
        <li class="list-group-item">Faculty Address:<b>@Model.Address</b></li>


        <li class="list-group-item">Faculty DOB:<b>@Model.FDOB</b></li>
        <li class="list-group-item">Faculty Qualification:<b>@Model.Qualification</b></li>
        



    </ul>

    }



</div>
<br />
<br />
<table>
    <tr>
        <td>
            <p><a class="btn btn-default" href="~/Home/GetStudents">View Students</a></p>
        </td>
        <td>
            <p><a class="btn btn-default" href="~/Home/Index">Back To Home Page</a></p>
        </td>
    </tr>
</table>
